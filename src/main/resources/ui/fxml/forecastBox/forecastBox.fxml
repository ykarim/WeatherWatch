<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="15.0" type="javafx.scene.layout.VBox"
         xmlns="http://javafx.com/javafx/8.0.121">
    <children>
        <Label fx:id="lbl_day" text="Label"/>
        <AnchorPane fx:id="anchorPane_icon" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                    minHeight="0.0" minWidth="0.0" VBox.vgrow="ALWAYS">
            <children>
                <ImageView fx:id="imgView_icon" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0"
                           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </AnchorPane>
        <HBox alignment="CENTER_LEFT" spacing="10.0">
            <children>
                <Label fx:id="lbl_highTemp" text="Label">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="lbl_lowTemp" alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Label"
                       textAlignment="RIGHT" textFill="GREY">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
            </children>
            <VBox.margin>
                <Insets left="10.0" right="10.0"/>
            </VBox.margin>
        </HBox>
    </children>
</fx:root>
